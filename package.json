{
  "name": "dno-api",
  "version": "1.1.0",
  "description": "API for DataNetworkOverlock project",
  "main": "index.js",
  "scripts": {
    "start": "node --env-file=.env dist/index",
    "dev": "npm run build && node --env-file=.env dist/index",
    "clean": "rimraf dist && rimraf coverage",
    "prebuild": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "audit": "npm audit",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.ts\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.ts\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.ts\"",
    "predeploy": "npm install --omit=dev && npm run build",
    "deploy": "pm2 start \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataNetworkOverlock/dno-api.git"
  },
  "author": "miklegonza",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DataNetworkOverlock/dno-api/issues"
  },
  "homepage": "https://github.com/DataNetworkOverlock/dno-api#readme",
  "dependencies": {
    "express": "^4.18.2",
    "inversify": "^6.0.2",
    "module-alias": "^2.2.3",
    "mysql": "^2.18.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.24",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "_moduleAliases": {
    "@infrastructure": "dist/infrastructure",
    "@application": "dist/application",
    "@domain": "dist/domain",
    "@api": "dist/infrastructure/driving-adapters/api-rest"
  }
}
